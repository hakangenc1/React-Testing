{
  "name": "React-ES6-Webpack-Boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/SaniaLs4Life/React-ES6-Webpack-Boilerplate",
  "author": "Hakan Genc <gnchakan1@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "test": "jest --watchAll",
    "unittest": "jest __tests__/App.test.js -t",
    "build": "webpack --mode production",
    "cypress": "cypress open",
    "test:coverage": "jest --coverage",
    "open:coverage": "start ./coverage/index.html"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.0",
    "cypress": "^3.8.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}